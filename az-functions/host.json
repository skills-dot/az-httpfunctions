{
    "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "extensions": {
      "http": {
        "customHeaders": {
          "Access-Control-Expose-Headers": "*",
          "Access-Control-Allow-Origin": "https://httptriggerfunction.azurewebsites.net"
        }
      }
    }
  }
}